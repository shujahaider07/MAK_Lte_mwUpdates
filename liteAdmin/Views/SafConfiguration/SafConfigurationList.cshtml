@model IEnumerable<BusinessLogic.SafConfiguration>

@{
    ViewData["Title"] = "SafConfigurationList";
}

<h1>SafConfigurationList</h1>

<p>
    <small><a asp-area="" asp-action="SafConfiguration" style="float: left; margin-right: 10px; margin-bottom: 5px; width: 75px;"> <i class="btn btn-block btn-primary " style="margin-top"><span>Create</span></i></a></small>
</p>
<table class="table">
    <thead>
        <tr>
